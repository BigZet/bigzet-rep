dens=300
x=seq(-3, 5, length=dens+1)
y=seq(-5, 5, length=dens+1)
z=seq(-5, 5, length=dens+1)
f5 = function(x, y, z) {
R=2
r=1
M=matrix(numeric(0), ncol=3)
for (xi in x){
for (yi in y) {
for (zi in z) {
if(abs((xi^2+yi^2+zi^2+R^2-r^2)^2-4*R^2*(xi^2+yi^2))<50e-2){
M = rbind(M, c(xi, yi, zi))
# print("HI")
}
}
}
}
return(M)
}
k=f5(x, y, z)
points3d(k)
